{
  "name": "divi-error-overlay",
  "version": "1.0.0",
  "description": "An overlay for displaying stack frames.",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build:prod",
    "start": "cross-env NODE_ENV=development node build.js --watch",
    "test": "cross-env NODE_ENV=test jest",
    "build": "cross-env NODE_ENV=development node build.js",
    "build:prod": "cross-env NODE_ENV=production node build.js"
  },
  "repository": "elegantthemes/create-divi-extension",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elegantthemes/create-divi-extension/issues"
  },
  "keywords": [
    "overlay",
    "syntax",
    "error",
    "red",
    "box",
    "redbox",
    "crash",
    "warning"
  ],
  "author": "Joe Haddad <timer150@gmail.com>",
  "files": [
    "lib/index.js"
  ],
  "devDependencies": {
    "@babel/code-frame": "7.18.6",
    "@babel/core": "7.18.10",
    "@babel/runtime": "7.18.9",
    "anser": "2.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-preset-divi-extension": "^1.0.0",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.22.0",
    "eslint-config-divi-extension": "^1.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "flow-bin": "^0.184.0",
    "html-entities": "2.3.3",
    "jest": "28.1.3",
    "jest-fetch-mock": "3.0.3",
    "object-assign": "4.1.1",
    "promise": "8.1.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "settle-promise": "1.0.0",
    "source-map": "0.7.4",
    "webpack": "^5.74.0"
  },
  "jest": {
    "setupFiles": [
      "./src/__tests__/setupJest.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "setupJest.js"
    ]
  }
}
